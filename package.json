{
  "name": "scripts-manager",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "node src/app.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "build": "tsc",
    "prisma:migrate": "prisma migrate dev --name dev",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate"
  },
  "keywords": [
    "scripts",
    "manager",
    "scripts-manager",
    "orchestrator",
    "orchestrator-scripts",
    "orchestrator-scripts-manager"
  ],
  "author": "Patrick Enderson",
  "license": "MIT",
  "description": "This project is a scripts manager that allows you to manage your scripts in a more organized way.",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1"
  }
}
